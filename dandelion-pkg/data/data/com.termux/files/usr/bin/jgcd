# ðŸ“Š MÃ“DULO DE TELEMETRIA
show_telemetry() {
    local mem=$(free -m | awk '/Mem:/ {print $3}')
    echo -e "${DIM}[TELEMETRIA]: RAM_USED: ${mem}MB | PID: $$ | TTY: $(tty)${NC}"
}

# ðŸ“ BOOKMARKS (Favoritos)
target="$*"
case "$target" in
    "gbin")   target="$HOME/gbin" ;;
    "mods")   target="$HOME/Mini-sys" ;;
    "down")   target="/sdcard/Download" ;;
    "term")   target="$HOME" ;;
esac

# âš™ï¸ MOTOR DE NAVEGAÃ‡ÃƒO
if [[ "${BASH_SOURCE[0]}" == "${0}" ]]; then
    echo -e "${R}âš ï¸ ERRO: Use 'jgcd' como alias!${NC}"
    return 1 2>/dev/null || exit 1
fi

if [ -z "$target" ]; then
    cd ~ || return
    echo -e "${P}ðŸš€ Salto: ${C}ï€•  Home (~)${NC}"
    show_telemetry ; echo -e "${DIM}  â””â”€ AI: $(get_quote)${NC}"
    return
fi

if [ -d "$target" ]; then
    cd "$target" || return
    echo -e "${G}ðŸš€ Pouso: ${B}ï„•  $(pwd)${NC}"
    show_telemetry ; echo -e "${DIM}  â””â”€ AI: $(get_quote)${NC}"
    [ -f "$HOME/gbin/lyr" ] && bash "$HOME/gbin/lyr"
    return
fi

# ðŸ”® FUZZY MATCHING (Busca cega)
fuzzy_target=$(echo "$target" | sed 's/ /*/g')
guess=$(find . -maxdepth 2 -type d -iname "*${fuzzy_target}*" 2>/dev/null | head -n 1)
guess="${guess#./}"

if [ -n "$guess" ] && [ -d "$guess" ]; then
    echo -e "${Y}ðŸ¤” Quis dizer: ${C}ï„•  ${guess}${Y}?${NC}"
    cd "$guess" || return
    echo -e "${G}ðŸš€ Pouso: ${B}ï„•  $(pwd)${NC}"
    show_telemetry ; echo -e "${DIM}  â””â”€ AI: $(get_quote)${NC}"
    [ -f "$HOME/gbin/lyr" ] && bash "$HOME/gbin/lyr"
    return
fi

# ðŸ’€ TRATAMENTO DE ERRO
echo -e "${R}ðŸ’€ ERRO: Pasta '${target}' nÃ£o existe.${NC}"

